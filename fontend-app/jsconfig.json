{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": ["es6"],
    "jsx": "react-native",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "assets": ["assets"],
      "assets/*": ["assets/*"],
      "components": ["components"],
      "components/*": ["components/*"],
      "hooks": ["hooks"],
      "hooks/*": ["hooks/*"],
      "i18n": ["i18n"],
      "i18n/*": ["i18n/*"],
      "navigator": ["navigator"],
      "navigator/*": ["navigator/*"],
      "store": ["store"],
      "store/*": ["store/*"],
      "screens": ["screens"],
      "screens/*": ["screens/*"],
      "utils": ["utils"],
      "utils/*": ["utils/*"],
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
